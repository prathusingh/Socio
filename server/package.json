    "crypto": "^1.0.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "googleapis": "^38.0.0",
    "i": "^0.3.6",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.13",
    "node-mailer": "^0.1.1",
    "node-pre-gyp": "^0.12.0",
    "passport": "^0.4.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
